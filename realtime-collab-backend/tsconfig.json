{
    "compilerOptions": {
      "target": "ES2020",
      "typeRoots": ["./node_modules/@types", "./src/types"],
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }